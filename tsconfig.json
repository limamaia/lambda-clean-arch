{
    "compilerOptions": {
      "lib": ["es2021"],
      "outDir": "dist",
      "rootDir": "./",
      "module": "commonjs",
      "target": "es2021",
  
      "strict": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node"
    }
  }